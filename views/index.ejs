<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>
  <h1>
    <%= title %>
  </h1>
  <p>Welcome to <%= title %>
  </p>
  <% if(user) { %>
    <p>Id: <%= user.id %>
    </p>
    <p>Display Name: <%= user.displayName %>
    </p>
    <img src="<%= user.pictureUrl %>" width="200">
    <% } else { %>
      <a href="/login/line"><img src="/images/btn_login_base.png"></a>
      <% } %>
</body>

</html>